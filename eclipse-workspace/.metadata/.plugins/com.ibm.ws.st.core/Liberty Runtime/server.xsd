    </xsd:complexType> 
    <xsd:complexType name="com.ibm.ws.security.authentication.tai"> 
        <xsd:annotation> 
            <xsd:documentation>Controla a operação do Trust Association Interceptor (TAI).</xsd:documentation> 
            <xsd:appinfo> 
                <ext:label>Trust Association Interceptor</ext:label> 
            </xsd:appinfo> 
        </xsd:annotation> 
        <xsd:sequence> 
            <xsd:element name="interceptors" minOccurs="0" maxOccurs="unbounded" type="com.ibm.ws.security.authentication.tai.interceptor-factory"> 
            </xsd:element> 
        </xsd:sequence> 
        <xsd:attribute name="invokeForUnprotectedURI" use="optional" default="false" type="booleanType"> 
            <xsd:annotation> 
                <xsd:documentation>Controla se o TAI é chamado para um URI não protegido.</xsd:documentation> 
                <xsd:appinfo> 
                    <ext:label>Chamar TAI para URI Não Protegido</ext:label> 
                </xsd:appinfo> 
            </xsd:annotation> 
        </xsd:attribute> 
        <xsd:attribute name="failOverToAppAuthType" use="optional" default="false" type="booleanType"> 
            <xsd:annotation> 
                <xsd:documentation>Permitir que um interceptor efetue fallback no mecanismo de autenticação do aplicativo.</xsd:documentation> 
                <xsd:appinfo> 
                    <ext:label>Permitir Fallback para Autenticação do Aplicativo</ext:label> 
                </xsd:appinfo> 
            </xsd:annotation> 
        </xsd:attribute> 
        <xsd:attribute name="continueAfterUnprotectedURI" use="optional" default="false" type="booleanType"> 
            <xsd:annotation> 
                <xsd:documentation>Continua a autenticação após o TAI para um URI desprotegido</xsd:documentation> 
                <xsd:appinfo> 
                    <ext:label>Continuar após o TAI para um URI desprotegido</ext:label> 
                </xsd:appinfo> 
            </xsd:annotation> 
        </xsd:attribute> 
        <xsd:anyAttribute processContents="skip"> 
        </xsd:anyAttribute> 
    </xsd:complexType> 
